<Window x:Class="MiniProject_Batch_Rename.UpdateMoveArgsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniProject_Batch_Rename"
        mc:Ignorable="d"
        Title="UpdateMoveArgsDialog" Height="195.561" Width="389.953" Loaded="Window_Loaded">
    <Canvas>
        <Label Content="amount" Canvas.Left="71" Canvas.Top="6"/>
        <TextBox Name="newNameBox" Width="228" Height="25"
                 Text="{Binding amount}" Canvas.Left="71" Canvas.Top="37"/>
        <Button Name="saveButton" IsDefault="True" Content="Save" Height="30" Width="100" Canvas.Left="71" Canvas.Top="81" Click="OkButton_Click"/>
        <Button Name="cancelButton" IsCancel="True" Content="Cancel" Height="30" Width="100" Canvas.Left="199" Canvas.Top="81" />
    </Canvas>
</Window>
