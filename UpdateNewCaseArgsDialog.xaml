<Window x:Class="MiniProject_Batch_Rename.UpdateNewCaseArgsDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MiniProject_Batch_Rename"
        mc:Ignorable="d"
        Title="UpdateNewCaseArgsDialog" Loaded="Window_Loaded" Height="195.561" Width="389.953">
    <Canvas>
        <Label Content="OldName:" Canvas.Left="71" Canvas.Top="6"/>
        <TextBox Name="newNameBox" Width="228" Height="25"
                 Text="{Binding newName}" Canvas.Left="71" Canvas.Top="37"/>
        <Button Name="saveButton" Content="Save" Height="30" Width="100" Canvas.Left="71" Canvas.Top="81"/>
        <Button Name="cancelButton" Content="Cancel" Height="30" Width="100" Canvas.Left="199" Canvas.Top="81" />
    </Canvas>
</Window>
